        <structure name="Box" id="90" encoding="ISO_8859-1:1987" endian="little" signed="no">
            <number name="Xmin" id="200" fillcolor="72FCD5" type="float" length="8"/>
            <number name="Ymin" id="201" fillcolor="00FA92" type="float" length="8"/>
